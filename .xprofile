#!/bin/sh

sys_type="$(cat /sys/class/dmi/id/chassis_type)"

if [ "$sys_type" -eq 3 ] # Desktop
then
    echo "Desktop"
elif [ "$sys_type" -eq 10 ] # Notebook
then
    echo "Notebook"

    nm-applet &
else
    echo "Unknown system"
fi

xrandr --setprovideroutputsource modesetting NVIDIA-0
xrandr --auto
picom -f &
nitrogen --restore &
# xautolock -time $LOCK_TIME -locker lock -corners +00- &
