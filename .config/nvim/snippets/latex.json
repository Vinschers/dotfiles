{
    "Basic Template": {
        "prefix": "article",
        "body": [
            "\\documentclass[a4paper]{article}",
            "\\usepackage[utf8]{inputenc}",
            "\\usepackage[T1]{fontenc}",
            "\\usepackage{textcomp}",
            "\\usepackage[dutch]{babel}",
            "\\usepackage{amsmath, amssymb}",


            "% figure support",
            "\\usepackage{import}",
            "\\usepackage{xifthen}",
            "\\pdfminorversion=7",
            "\\usepackage{pdfpages}",
            "\\usepackage{transparent}",
            "\\newcommand{\\incfig}[1]{%",
	            "\t\\def\\svgwidth{\\columnwidth}",
	            "\t\\import{./figures/}{#1.pdf_tex}",
            "}",

            "\\pdfsuppresswarningpagegroup=1",

            "\\begin{document}",
	            "\t$0",
            "\\end{document}"
        ]
    },
    "Package": {
        "prefix": "pac",
        "body" : "\\usepackage[${1:options}]{${2:package}}$0"
    },
    "Less or equals": {
        "prefix": "<=",
        "body": "\\le"
    },
    "Greater or equals": {
        "prefix": ">=",
        "body": "\\ge"
    },
    "Curly less or equals": {
        "prefix": "c<=",
        "body": "\\preccurlyeq"
    },
    "Curly greater or equals": {
        "prefix": "c>=",
        "body": "\\succcurlyeq"
    },
    "Exists": {
        "prefix": "EE",
        "body": "\\exists"
    },
    "For all": {
        "prefix": "AA",
        "body": "\\forall"
    },
    "Leads to": {
        "prefix": ["leads", "~>"],
        "body": "\\leadsto"
    },
    "Implies": {
        "prefix": "=>",
        "body": "\\implies"
    },
    "Implied by": {
        "prefix": "<=",
        "body": "\\impliedby"
    },
    "If and only if": {
        "prefix": ["sse", "iff"],
        "body": "\\iff"
    },
    "Up simple arrow": {
        "prefix": ["uarrow", "-^"],
        "body": "\\uparrow"
    },
    "Up double arrow": {
        "prefix": ["Uarrow", "-^"],
        "body": "\\Uparrow"
    },
    "Down simple arrow": {
        "prefix": ["darrow", "-v"],
        "body": "\\downarrow"
    },
    "Down double arrow": {
        "prefix": ["Darrow", "=v"],
        "body": "\\Downarrow"
    },
    "<=>": {
        "prefix": "<=>",
        "body": "\\Leftrightarrow"
    },
    "Equivalent": {
        "prefix": "<->",
        "body": "\\leftrightarrow"
    },
    "Big O-plus": {
        "prefix": ["Oplus", "bigplus", "plusO"],
        "body": "\\bigoplus"
    },
    "To": {
        "prefix": "->",
        "body": "\\to"
    },
    "Maps to": {
        "prefix": "!>",
        "body": "\\mapsto"
    },
    "Set minus": {
        "prefix": "\\\\\\",
        "body": "\\setminus"
    },
    "Way greater": {
        "prefix": ">>",
        "body": "\\gg"
    },
    "Way less": {
        "prefix": "<<",
        "body": "\\ll"
    },
    "Approximately": {
        "prefix": "~~",
        "body": "\\sim"
    },
    "Set": {
        "prefix": "set",
        "body": "\\{$1\\} $0"
    },
    "Such that": {
        "prefix": "|",
        "body": "\\mid"
    },
    "Sub set": {
        "prefix": ["cc", "sub"],
        "body": "\\subset"
    },
    "Not in": {
        "prefix": ["nin", "notin"],
        "body": "\\not\\in"
    },
    "Intersection": {
        "prefix": ["inter", "inn"],
        "body": "\\cap"
    },
    "Union": {
        "prefix": ["UU", "union"],
        "body": "\\cup"
    },
    "All inter": {
        "prefix": ["Inn", "Inter"],
        "body": "\\bigcap_{${1:i} \\in ${2:I}} $0"
    },
    "All union": {
        "prefix": "UUU",
        "body": "\\bigcup_{${1:i} \\in ${2:I}} $0"
    },
    "Complex": {
        "prefix": "CC",
        "body": "\\mathbb{C}"
    },
    "Real": {
        "prefix": "RR",
        "body": "\\mathbb{R}"
    },
    "Rational": {
        "prefix": "QQ",
        "body": "\\mathbb{Q}"
    },
    "Integer": {
        "prefix": "ZZ",
        "body": "\\mathbb{Z}"
    },
    "Natural": {
        "prefix": "NN",
        "body": "\\mathbb{N}"
    },
    "Cases": {
        "prefix": "cases",
        "body": [
            "\\begin{cases}",
            "\t$1",
            "\\end{cases}"
        ]
    },
    "Big function": {
        "prefix": "bigfun",
        "body": [
            "\\begin{align*}",
            "\t$1: $2 &\\longrightarrow $3 \\\\\\",
            "\t$4 &\\longrightarrow $1($4) = $0",
            "\\end{align*}"
        ]
    },
    "Column vector": {
        "prefix": ["cvec", "colvec"],
        "body": "\\begin{pmatrix} ${1:x}_${2:1}\\\\\\ \\vdots\\\\\\ $1_${2:n} \\end{pmatrix}"
    },
    "Bar": {
        "prefix": "bar",
        "body": "\\overline{$1}$0"
    },
    "Hat": {
        "prefix": ["hat", "chapeu"],
        "body": "\\hat{$1}$0"
    },
    "Alter epsilon": {
        "prefix": ["epsilon", "episolon", "epsolon"],
        "body": "\\varepsilon"
    },
    "Alter phi": {
        "prefix": ["phi", "fi"],
        "body": "\\varphi"
    },
    "Alter theta": {
        "prefix": ["theta", "teta"],
        "body": "\\vartheta"
    },
    "Align*": {
        "prefix": "al",
        "body": [
            "\\begin{align*}",
            "\t$0",
            "\\end{align*}"
        ]
    },
    "fraction1": {
        "prefix": "f",
        "body": [
            "\\frac{$1}{$2} $0"
        ]
    },
    "fraction2": {
        "prefix": "/",
        "body": [
            "\\frac{$1}{$2} $0"
        ]
    },
    "Equals": {
        "prefix": "==",
        "body": "&= $1 \\\\"
    },
    "Not equals": {
        "prefix": "!=",
        "body": "\\neq"
    },
    "Ceil": {
        "prefix": "ceil",
        "body": "\\left\\lceil $1 \\right\\rceil $0"
    },
    "Teto": {
        "prefix": "teto",
        "body": "\\left\\lceil $1 \\right\\rceil $0"
    },
    "Floor": {
        "prefix": "floor",
        "body": "\\left\\lfloor $1 \\right\\rfloor $0"
    },
    "Piso": {
        "prefix": "piso",
        "body": "\\left\\lfloor $1 \\right\\rfloor $0"
    },
    "bold": {
        "prefix": "bold",
        "body": [
            "\\textbf{${1:${TM_SELECTED_TEXT}}} $0"
        ]
    },
    "italic": {
        "prefix": "italic",
        "body": [
            "\\textit{${1:${TM_SELECTED_TEXT}}} $0"
        ]
    },
    "underbrace": {
        "prefix": ["under", "anotu"],
        "body": [
            "\\underbrace{$1}_{$2} $0"
        ]
    },
    "overbrace": {
        "prefix": ["over", "anoto"],
        "body": [
            "\\overbrace{$1}_{$2} $0"
        ]
    },
    "underset": {
        "prefix": "underset",
        "body": [
            "\\underset{$1}{${2:${TM_SELECTED_TEXT}}} $0"
        ]
    },
    "overset": {
        "prefix": "overset",
        "body": [
            "\\overset{$1}{${2:${TM_SELECTED_TEXT}}} $0"
        ]
    },
    "thm": {
        "prefix": "thm",
        "body": [
            "\\begin{thm} $1 \\end{thm} $0"
        ]
    },
    "itemize": {
        "prefix": "itemize",
        "body": [
            "\\begin{itemize}",
            "\t\\item $1",
            "\\end{itemize}",
            "$0"
        ]
    },
    "enumerate": {
        "prefix": "enum",
        "body": [
            "\\begin{enumerate}",
            "\t\\item $1",
            "\\end{enumerate}",
            "$0"
        ]
    },
    "Environment": {
        "prefix": "beg",
        "body": [
            "\\begin{${1:name}}",
            "\t$0",
            "\\end{${1:name}}"
        ]
    },
    "Text": {
        "prefix": "txt",
        "body": [
            "\\text{$0}"
        ]
    },
    "Image": {
        "prefix": "img",
        "body": [
            "\\begin{figure}[H]",
            "\t\\centering",
            "\t\\includegraphics[width=0.5\\textwidth]{${1:${TM_SELECTED_TEXT}}}",
            "\t\\caption{${2:caption}\\label{fig:${3:sample}}}",
            "\\end{figure}",
            "$0"
        ]
    },
    "Table": {
        "prefix": "tab",
        "body": [
            "\\begin{table}[H]",
            "\t\\centering",
            "\t\\begin{tabular}{${1:ccc}}",
            "\t\t\\toprule",
            "\t\ta & b & c \\\\\\ \\midrule",
            "\t\t1 & 2 & 3 \\\\\\",
            "\t\t\\bottomrule",
            "\t\\end{tabular}",
            "\t\\caption{${2:caption}\\label{tab:${3:sample}}}",
            "\\end{table}",
            "$0"
        ]
    },
    "pmatrix": {
        "prefix": "pmat",
        "body": [
            "\\begin{pmatrix}",
            "$1",
            "\\end{pmatrix}",
            "$0"
        ]
    },
    "bmatrix": {
        "prefix": "bmat",
        "body": [
            "\\begin{bmatrix}",
            "$1",
            "\\end{bmatrix}",
            "$0"
        ]
    },
    "Infinity": {
        "prefix": "oo",
        "body": "\\infty"
    },
    "Integral": {
        "prefix": "int",
        "body": [
            "\\int_{${1:0}}^{${2:\\infty}} $4 \\,d$3 $0"
        ]
    },
    "Sum Full": {
        "prefix": "sumi",
        "body": [
            "\\sum_{${1:i}=${2:0}}^{${3:\\infty}} $0"
        ]
    },
    "undersetinline": {
        "prefix": "_",
        "body": [
            "_{$1}$0"
        ]
    },
    "parenthesis1": {
        "prefix": "lr(",
        "body": [
            "\\left($1\\right)"
        ]
    },
    "parenthesis2": {
        "prefix": "lr[",
        "body": [
            "\\left[$1\\right]"
        ]
    },
    "parenthesis3": {
        "prefix": "lr{",
        "body": [
            "\\left\\$1\\right\\}"
        ]
    },
    "parenthesis4": {
        "prefix": "lr|",
        "body": [
            "\\left|$1\\right|"
        ]
    },
    "average": {
        "prefix": "avg",
        "body": [
            "\\langle $1 \\rangle$0"
        ]
    },
    "Sum": {
        "prefix": "sum",
        "body": [
            "\\sum_{${1:n}=${2:1}}^{${3:\\infty}} $0"
        ]
    },
    "Taylor": {
        "prefix": "taylor",
        "body": [
            "\\sum_{${1:k}=${2:0}}^{${3:\\infty}} ${4:c_$1} (x-a)^$1 $0"
        ]
    },
    "Product": {
        "prefix": "prod",
        "body": "\\prod_{${1:n}=${2:1}}^{${3:\\infty}} $0"
    },
    "Limit": {
        "prefix": "lim",
        "body": "\\lim_{${1:n} \\to ${2:\\infty}} $0"
    },
    "Derivative": {
        "prefix": "D",
        "body": "\\frac{d $1}{d ${2:x}}"
    },
    "Partial derivative": {
        "prefix": "part",
        "body": "\\frac{\\partial $1}{\\partial ${2:x}}"
    },
    "Square root": {
        "prefix": "sq",
        "body": "\\sqrt{$1}"
    },
    "Nth root": {
        "prefix": "sqn",
        "body": "\\sqrt[$1]{$2}"
    },
    "Vector arrow": {
        "prefix": ["vector", "arrow", "vec"],
        "body": "\\overrightarrow{$1}"
    },
    "Fancy l": {
        "prefix": "ll",
        "body": "\\ell"
    },
    "Cross": {
        "prefix": "xx",
        "body": "\\times"
    },
    "Cdot": {
        "prefix": "**",
        "body": "\\cdot"
    },
    "Composition": {
        "prefix": ["o", "comp"],
        "body": "\\circ"
    },
    "Plus minus": {
        "prefix": ["pm", "plusminus"],
        "body": "\\pm"
    },
    "hbar": {
        "prefix": "hb",
        "body": [
            "\\hbar"
        ]
    },
    "section": {
        "prefix": "sec",
        "body": [
            "\\section{$1}",
            "$0"
        ]
    },
    "subsection": {
        "prefix": "ssec",
        "body": [
            "\\subsection{$1}",
            "$0"
        ]
    },
    "operatorname": {
        "prefix": "op",
        "body": [
            "\\operatorname{$1} $0"
        ]
    },
    "mathrm": {
        "prefix": "rm",
        "body": [
            "\\mathrm{$1} $0"
        ]
    }
}
