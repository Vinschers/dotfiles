{
    "custom/pacman": {
        "format": "{icon}{}",
        "return-type": "json",
        "format-icons": {
            "pending-updates": " ",
            "updated": ""
        },
        "exec-if": "which waybar-updates",
        "exec": "waybar-updates"
    },
    "tray": {
        "icon-size": 24,
        "tooltip": true,
        "spacing": 12
    },
    "clock": {
        "interval": 1,
        "format": "<b>  {:%H:%M:%S}</b>",
        "tooltip-format": "{:%d/%m/%Y}"
    }
}
